{
  "name": "apiagricolar",
  "version": "1.1.2",
  "description": "API ofical do site agricolar",
  "engines": {
    "node": "20.11.1"
  },
  "scripts": {
    "test": "tsc && NODE_DEBUG=test node --env-file=.env.test --test --experimental-test-coverage",
    "server:set": "node --watch --env-file=.env --import tsx ./src/graphql/schema/CUSTOMER/serverCustomer.mts",
    "server:farmer": "node --watch --env-file=.env --import tsx ./src/graphql/schema/FARMER/serverFarmer.mts",
    "server:customer": "node --watch --env-file=.env --import tsx ./src/graphql/schema/SET/serverSet.mts",
    "server:http": "node --watch --env-file=.env --import tsx ./src/index.ts",
    "mainserver": "NODE_ENV=development NODE_DEBUG=server,main,database,set:*,CustomerServer,SetServer,FarmerServer node --watch --env-file=.env --import tsx ./src/graphql/server.mts",
    "dev": "NODE_ENV=development NODE_DEBUG=server,main,database,set:*,CustomerServer,SetServer,FarmerServer concurrently npm:server:*",
    "start": "NODE_ENV=production NODE_DEBUG=server,main,database,set:* node --env-file=.env ./dist/bundle.js",
    "up": "pm2 start ecosystem.config.js",
    "build": "rm -rf ./bundle && tsc",
    "graphql:compose": "rover supergraph compose --config ./router/supergraph.yaml --output ./router/prod-schema.graphql",
    "graphql:start": "npm run graphql:compose && exec ./router/router --supergraph=./router/prod-schema.graphql"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/delciocapolo/APIAgricolar"
  },
  "keywords": [
    "API",
    "Typescript",
    "Javascript",
    "Nodejs",
    "Jest",
    "GraphQL",
    "Express",
    "Zod"
  ],
  "author": "Delcio Capolo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delciocapolo/APIAgricolar/issues"
  },
  "dependencies": {
    "@apollo/gateway": "^2.7.8",
    "@apollo/server": "^4.10.2",
    "@apollo/subgraph": "^2.7.5",
    "@prisma/client": "^5.12.1",
    "@prisma/internals": "^5.12.1",
    "@types/make-fetch-happen": "^10.0.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-var": "^7.4.1",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "make-fetch-happen": "^13.0.1",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "pm2": "^5.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.2",
    "@types/supertest": "^6.0.2",
    "concurrency": "^0.1.4",
    "copy-webpack-plugin": "^12.0.2",
    "coverage": "^0.4.1",
    "loadtest": "^8.0.9",
    "prisma": "^5.12.1",
    "supertest": "^6.3.4",
    "ts-loader": "^9.5.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^1.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}

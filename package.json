{
  "name": "apiagricolar",
  "version": "1.1.2",
  "description": "API ofical do site agricolar",
  "engines": {
    "node": "20.11.1"
  },
  "type": "module",
  "scripts": {
    "test": "npx tsc && NODE_DEBUG=test node --env-file=.env.test --test --experimental-test-coverage",
    "dev": "NODE_DEBUG=server,main,database,set:* node --watch --env-file=.env --import tsx ./src/index.mts",
    "bundle": "tsup .",
    "start": "NODE_DEBUG=server,main,database,set:* node --watch --env-file=.env ./bundle/src/index.mjs",
    "up": "pm2 start npm -- start --watch --ignore-watch=node_modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delciocapolo/APIAgricolar"
  },
  "keywords": [
    "API",
    "Typescript",
    "Javascript",
    "Nodejs",
    "Jest",
    "GraphQL",
    "Express",
    "Zod"
  ],
  "author": "Delcio Capolo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/delciocapolo/APIAgricolar/issues"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@apollo/subgraph": "^2.7.5",
    "@prisma/client": "^5.12.1",
    "@prisma/internals": "^5.12.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-var": "^7.4.1",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "ngrok": "^5.0.0-beta.2",
    "node-fetch": "^3.3.2",
    "pm2": "^5.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.2",
    "@types/supertest": "^6.0.2",
    "coverage": "^0.4.1",
    "loadtest": "^8.0.9",
    "prisma": "^5.12.1",
    "supertest": "^6.3.4",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
